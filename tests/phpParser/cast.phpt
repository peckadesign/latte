<?php

// Casts

declare(strict_types=1);

use Tester\Assert;

require __DIR__ . '/../bootstrap.php';

$test = <<<'XX'
	(array)   $a,
	(bool)    $a,
	(float)   $a,
	(int)     $a,
	(object)  $a,
	(string)  $a,
	XX;

$node = parseCode($test);

Assert::same(
	loadContent(__FILE__, __COMPILER_HALT_OFFSET__),
	exportNode($node),
);

__halt_compiler();
